  {
    "name": "Asana Expand-Comments",
    "version": "0.1",
    "description": "Expand all comments on an Asana task",
    "permissions": ["activeTab", "storage"],
    "commands": {
      "toggle-feature-foo": {
	"suggested_key": {
	  "default": "Ctrl+Shift+Y",
	  "mac": "Command+Shift+Y"
	},
	"description": "Expand all comments"
      }
    },
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "manifest_version": 2
  }
